{
    "devCommand": "uvicorn server:app --host 0.0.0.0 --port 8000",
    "builds": [
        {
            "src": "server.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "450mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server.py"
        }
    ]
}